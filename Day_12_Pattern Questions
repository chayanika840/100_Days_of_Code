//Print this pattern
****

***

**

*

package com.company;

public class Main {

    public static void main(String[] args) {
	// write your code here
        int n=4;
        for(int i=n;i>0;i--){
            for(int j=0;j<i;j++){
                System.out.print("*");
            }
            System.out.println("\n");
        }

    }
}




